    "debug": "^2.6.9",
    "dotenv": "^5.0.1",
    "express": "^4.16.1",
    "express-jwt": "^5.3.1",
    "http-errors": "^1.6.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mssql": "^5.1.0",
    "mysql2": "^1.5.3",
    "pbkdf2": "^3.0.14",
